{
  "compilerOptions": {
    "target": "ES2015",
    "module": "CommonJS",
    "lib": ["es2015", "dom", "dom.iterable"],
    "strict": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "noUnusedLocals": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "noEmit": true
  }
}
